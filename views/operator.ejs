                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Total Student</label>
                        <input type="text" class="form-control" id="inputStudent" aria-describedby="emailHelp"
                            placeholder="Number of Student" />
                    </div>
                    <button id="submit" type="submit" class="btn btn-primary">Submit</button>
