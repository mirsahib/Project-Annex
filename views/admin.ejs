  <body>
    <nav class="navbar navbar-dark sticky-top flex-md-nowrap p-0 admin_nav">
      <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#">Project Annex</a>
    </nav>

    <div class="container-fluid">
      <div class="row">
        <nav class="col-md-2 d-none d-md-block admin_sidebar sidebar">
          <div class="sidebar-sticky">
            <ul class="nav flex-column">
              <li class="nav-item">
                <a class="nav-link" href="#">
                  <span data-feather="home"></span>
                  Yearwise Distribution
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">
                  <span data-feather="file"></span>
                  Schoolwise Distribution
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">
                  <span data-feather="shopping-cart"></span>
                  Majorwise Distribution
                </a>
              </li>
            </ul>
          </div>
        </nav>

        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
          <div class="mb-3 border-bottom">
            <h1 class="h2">YearWise Distribution</h1>
          </div>

          <!--<div
            class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom"
          >
            <h1 class="h2">YearWise Distribution</h1>
            <div class="btn-toolbar mb-2 mb-md-0">
              <div class="btn-group mr-2">
                <button class="btn btn-sm btn-outline-secondary">Share</button>
                <button class="btn btn-sm btn-outline-secondary">Export</button>
              </div>
              <button class="btn btn-sm btn-outline-secondary dropdown-toggle">
                <span data-feather="calendar"></span>
                This week
              </button>
            </div>
          </div>-->
          <form>
            <div class="container">
              <div class="row">
                <div class="col-sm">
                  <label for="exampleFormControlSelect1">Year</label>
                  <select class="form-control" id="yearSelect">
                    <option value="null" disabled selected>Select Year</option>
                    <option>2019</option>
                    <option>2018</option>
                    <option>2017</option>
                    <option>2016</option>
                  </select>
                </div>
                <div class="col-sm">
                  <label for="exampleFormControlSelect1">Semester Name</label>
                  <select class="form-control" id="semesterSelect">
                    <option value="null" disabled selected
                      >Select Semester</option
                    >
                    <option>Autumn</option>
                    <option>Spring</option>
                    <option>Summer</option>
                  </select>
                </div>
                <!--<button type="submit"> will refresh the page
                <button type="button"> won't refresh the page
                -->
                <div class="col-sm">
                  <button
                    id="yearSubmit"
                    style="margin-top: 30px;"
                    type="button"
                    class="btn btn-primary"
                  >
                    Submit
                  </button>
                </div>
              </div>
            </div>
          </form>
          <div class="pie-chart-container">
            <canvas id="yearChart"></canvas>
          </div>
          <div class="form-group">
            <label for="comment">Comment:</label>
            <textarea class="form-control" rows="5" id="yearComment"></textarea>
          </div>

          <!--School wise distribution-->
          <div class="mb-3 border-bottom">
            <h1 class="h2">Schoolwise Distribution</h1>
          </div>
          <form>
            <div class="container">
              <div class="row">
                <div class="col-sm">
                  <label for="exampleFormControlSelect1">School Name</label>
                  <select class="form-control" id="schoolName">
                    <option disabled selected>Select School Name</option>
                    <!--
                    <option>Autumn(Fall)</option>
                    <option>Spring</option>
                    <option>Summer</option>
                    -->
                  </select>
                </div>
                <div class="col-sm">
                  <label for="exampleFormControlSelect1">Year Span</label>
                  <select class="form-control" id="yearSpan">
                    <option disabled selected>Select Year Span</option>
                    <option>Last 1 Year</option>
                    <option>Last 2 Year</option>
                    <option>Last 3 Year</option>
                  </select>
                </div>
                <div class="col-sm">
                  <button
                    style="margin-top: 30px;"
                    id="schoolSubmit"
                    type="button"
                    class="btn btn-primary"
                  >
                    Submit
                  </button>
                </div>
              </div>
            </div>
          </form>
          <div class="pie-chart-container">
            <canvas id="SchoolChart"></canvas>
          </div>
          <div class="form-group">
            <label for="comment">Comment:</label>
            <textarea
              class="form-control"
              rows="5"
              id="schoolComment"
            ></textarea>
          </div>

          <!--Major Wise distribution-->
          <div class="mb-3 border-bottom">
            <h1 class="h2">Majorwise Distribution</h1>
          </div>
          <form>
            <div class="container">
              <div class="row">
                <div class="col-sm">
                  <label for="exampleFormControlSelect1">Major Name</label>
                  <select class="form-control" id="majorName">
                    <option disabled selected>Select School Name</option>
                    <!--
                    <option>Autumn(Fall)</option>
                    <option>Spring</option>
                    <option>Summer</option>
                    -->
                  </select>
                </div>
                <div class="col-sm">
                  <label for="exampleFormControlSelect1">Year Span</label>
                  <select class="form-control" id="majorYearSpan">
                    <option disabled selected>Select Year Span</option>
                    <option>Last 1 Year</option>
                    <option>Last 2 Year</option>
                    <option>Last 3 Year</option>
                  </select>
                </div>
                <div class="col-sm">
                  <button
                    style="margin-top: 30px;"
                    id="majorSubmit"
                    type="button"
                    class="btn btn-primary"
                  >
                    Submit
                  </button>
                </div>
              </div>
            </div>
          </form>
          <div class="pie-chart-container">
            <canvas id="majorChart"></canvas>
          </div>
          <div class="form-group">
            <label for="comment">Comment:</label>
            <textarea
              class="form-control"
              rows="5"
              id="majorComment"
            ></textarea>
            <button
              style="margin-top: 30px;"
              id="generateReport"
              type="button"
              class="btn btn-primary"
            >
              Submit
            </button>
          </div>
        </main>
      </div>
    </div>

